# ============================================
# NoOrg Multi-Agent Framework Configuration
# ============================================

# ----------------
# Core Configuration
# ----------------
NODE_ENV=production
DEBUG=false
LOG_LEVEL=info

# ----------------
# OpenAI Configuration
# ----------------
OPENAI_API_KEY=sk-proj-your-key-here
DEFAULT_MODEL=gpt-4o
MAX_TOKENS=8000
TEMPERATURE=0.7
TOP_P=1.0
FREQUENCY_PENALTY=0.0
PRESENCE_PENALTY=0.0

# ----------------
# Coordinator Configuration
# ----------------
COORDINATOR_NAME=Production Coordinator
COORDINATION_STRATEGY=hybrid
MAX_CONCURRENT_TASKS=10
ENABLE_AUTO_RETRY=true
MAX_RETRY_ATTEMPTS=3
TASK_PRIORITY_QUEUE=true
USE_AGENT_SPECIALIZATION=true
MONITOR_AGENT_PERFORMANCE=true

# ----------------
# State Management
# ----------------
STATE_FILE_PATH=./data/coordinator-state.json
AUTO_SAVE_ENABLED=true
AUTO_SAVE_INTERVAL=60000
STATE_PERSISTENCE_BACKEND=memory
STATE_ENCRYPTION_ENABLED=false
STATE_ENCRYPTION_KEY=

# ----------------
# Agent Configuration
# ----------------
AGENT_HEALTH_CHECK_INTERVAL=30000
AGENT_MAX_IDLE_TIME=300000
AGENT_AUTO_SCALING=false
AGENT_MIN_INSTANCES=1
AGENT_MAX_INSTANCES=10

# ----------------
# Monitoring & Metrics
# ----------------
ENABLE_METRICS=true
METRICS_PORT=9090
DASHBOARD_PORT=3000
METRICS_RETENTION_DAYS=30
METRICS_COLLECTION_INTERVAL=5000
ENABLE_HEALTH_CHECKS=true
HEALTH_CHECK_TIMEOUT=5000
ALERT_WEBHOOK_URL=

# ----------------
# Storage Configuration
# ----------------
STORAGE_BACKEND=memory
STORAGE_FILE_PATH=./data/storage
STORAGE_DATABASE_URL=
STORAGE_MAX_SIZE=1GB
STORAGE_ENCRYPTION_ENABLED=false
STORAGE_COMPRESSION_ENABLED=true

# ----------------
# Event System
# ----------------
EVENT_PERSISTENCE_ENABLED=false
EVENT_HISTORY_SIZE=1000
EVENT_MAX_LISTENERS=100
EVENT_EMIT_TIMEOUT=5000

# ----------------
# Messaging System
# ----------------
MESSAGE_QUEUE_SIZE=1000
MESSAGE_TIMEOUT=30000
MESSAGE_RETRY_ATTEMPTS=3
MESSAGE_PERSISTENCE_ENABLED=false

# ----------------
# Security Configuration
# ----------------
ENABLE_AUTHENTICATION=false
JWT_SECRET=your-jwt-secret-here
API_KEY=your-api-key-here
ENABLE_RATE_LIMITING=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60
ENABLE_AUDIT_LOGGING=true
AUDIT_LOG_RETENTION_DAYS=90

# ----------------
# Database Configuration
# ----------------
DATABASE_URL=postgresql://localhost:5432/noorg
DATABASE_POOL_SIZE=10
DATABASE_CONNECTION_TIMEOUT=5000
DATABASE_QUERY_TIMEOUT=30000
ENABLE_DATABASE_MIGRATIONS=false

# ----------------
# Redis Configuration (for caching)
# ----------------
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_KEY_PREFIX=noorg:
REDIS_TTL=3600

# ----------------
# Performance Configuration
# ----------------
WORKER_THREADS=4
MAX_MEMORY_MB=2048
ENABLE_CACHING=true
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
ENABLE_PROFILING=false

# ----------------
# Network Configuration
# ----------------
HTTP_PORT=3000
HTTPS_PORT=3443
ENABLE_HTTPS=false
SSL_CERT_PATH=
SSL_KEY_PATH=
TRUSTED_PROXIES=

# ----------------
# Health Monitoring
# ----------------
HEALTH_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000
MIN_SUCCESS_RATE=90
MAX_ERROR_RATE=10
MAX_RESPONSE_TIME=5000
ENABLE_CIRCUIT_BREAKER=true
CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
CIRCUIT_BREAKER_RECOVERY_TIMEOUT=60000

# ----------------
# External Integrations
# ----------------
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=
EMAIL_SMTP_HOST=
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USER=
EMAIL_SMTP_PASSWORD=
EMAIL_FROM_ADDRESS=noreply@noorg.local

# ----------------
# Development Configuration
# ----------------
HOT_RELOAD_ENABLED=false
SOURCE_MAPS_ENABLED=false
VERBOSE_LOGGING=false
ENABLE_DEBUG_ENDPOINTS=false

# ----------------
# Backup and Recovery
# ----------------
ENABLE_AUTO_BACKUP=false
BACKUP_INTERVAL_HOURS=24
BACKUP_RETENTION_DAYS=7
BACKUP_STORAGE_PATH=./backups
ENABLE_ENCRYPTED_BACKUPS=false

# ----------------
# Feature Flags
# ----------------
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_ADVANCED_ANALYTICS=false
ENABLE_REAL_TIME_COLLABORATION=false
ENABLE_VOICE_INTERFACE=false

# ----------------
# Custom Agent Configuration
# ----------------
CUSTOM_AGENT_SETTINGS={}
MAX_CUSTOM_AGENTS=5
CUSTOM_AGENT_TIMEOUT=60000

# ----------------
# Logging Configuration
# ----------------
LOG_FILE=./logs/noorg.log
LOG_MAX_SIZE=10MB
LOG_MAX_FILES=5
LOG_FORMAT=json
ENABLE_CONSOLE_LOGGING=true
ENABLE_FILE_LOGGING=true

# ----------------
# API Configuration
# ----------------
API_VERSION=v1
API_PREFIX=/api
ENABLE_CORS=true
CORS_ORIGINS=*
API_RATE_LIMIT=1000
API_TIMEOUT=30000

# ----------------
# Deployment Configuration
# ----------------
DEPLOYMENT_ENVIRONMENT=production
DOCKER_IMAGE_TAG=latest
KUBERNETES_NAMESPACE=noorg
HELM_CHART_VERSION=1.0.0
TERRAFORM_WORKSPACE=production

# ----------------
# Monitoring Integrations
# ----------------
PROMETHEUS_ENABLED=false
PROMETHEUS_PORT=9091
GRAFANA_ENABLED=false
GRAFANA_PORT=3001
GRAFANA_ADMIN_PASSWORD=
DATADOG_API_KEY=
NEW_RELIC_LICENSE_KEY=

# ----------------
# Advanced Features
# ----------------
ENABLE_ACTIVE_INFERENCE=false
ENABLE_POMDP_MODELS=false
ENABLE_FRACTAL_ARCHITECTURE=false
ENABLE_META_LEARNING=false
ENABLE_CROSS_DOMAIN_LEARNING=false

# ----------------
# Quality Assurance
# ----------------
ENABLE_CODE_QUALITY_CHECKS=true
ENABLE_SECURITY_SCANNING=true
ENABLE_PERFORMANCE_TESTING=true
ENABLE_ACCESSIBILITY_TESTING=false
TEST_COVERAGE_THRESHOLD=80

# ----------------
# Internationalization
# ----------------
DEFAULT_LOCALE=en-US
SUPPORTED_LOCALES=en-US,es-ES,fr-FR,de-DE
ENABLE_I18N=true
I18N_CACHE_TTL=3600

# ----------------
# Custom Extensions
# ----------------
ENABLE_CUSTOM_EXTENSIONS=false
EXTENSION_DIRECTORY=./extensions
MAX_EXTENSION_SIZE=10MB
EXTENSION_TIMEOUT=60000