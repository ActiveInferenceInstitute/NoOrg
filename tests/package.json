{
  "name": "multiagent-tests",
  "version": "1.0.0",
  "description": "Tests for multiagent coordination system",
  "main": "index.js",
  "scripts": {
    "test": "node runners/run_js.js",
    "test:unit": "node runners/run_js.js --test-type unit",
    "test:integration": "node runners/run_js.js --test-type integration",
    "test:system": "node runners/run_js.js --test-type system",
    "test:real-llm": "node runners/run_js.js --use-real-llm",
    "test:file": "node runners/run_js.js",
    "test:coverage": "node runners/run_js.js --report --coverage",
    "test:py": "python runners/run_py.py",
    "test:py:unit": "python runners/run_py.py --test-type unit",
    "test:py:integration": "python runners/run_py.py --test-type integration",
    "test:py:system": "python runners/run_py.py --test-type system",
    "test:py:coverage": "python runners/run_py.py --coverage --report",
    "test:structure": "python runners/run_py.py --markers structure"
  },
  "keywords": [
    "test",
    "mocha",
    "chai",
    "typescript",
    "multiagent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@types/sinon": "^10.0.15",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "sinon": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  }
} 