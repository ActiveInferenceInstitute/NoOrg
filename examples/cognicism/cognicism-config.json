{
  "framework": {
    "version": "1.0.0",
    "domain": "Cognicism Framework Implementation",
    "objective": "Implement a practical demonstration of the Cognicism framework for collaborative knowledge generation",
    "constraints": [
      "Must showcase Iris model, FourThought process, Semantic Ledger, and Å¦rust dynamics",
      "Must integrate advanced trust mechanisms and source evaluation",
      "Must support real-time visualization and analytics",
      "Must enable community consensus and belief staking mechanisms"
    ],
    "targetUsers": [
      "Research communities",
      "Distributed knowledge networks",
      "Collaborative decision-making groups",
      "Democratic AI systems",
      "Academic institutions",
      "Policy-making organizations"
    ],
    "enableAdvancedFeatures": true,
    "enableRealTimeAnalytics": true
  },
  "llm": {
    "defaultModel": "gpt-4o",
    "fallbackModel": "gpt-3.5-turbo-16k",
    "tokenLimitThreshold": 8000,
    "defaultMaxTokens": 2000,
    "temperature": 0.7,
    "topP": 0.9,
    "frequencyPenalty": 0.0,
    "presencePenalty": 0.0,
    "timeoutMs": 30000
  },
  "trust": {
    "initialTrustValue": 0.5,
    "trustUpdateRate": 0.1,
    "socialProofWeight": 0.2,
    "prophetWeight": 0.15,
    "temporalWeight": 0.05,
    "consensusWeight": 0.1,
    "enableAdvancedTrustCalculation": true
  },
  "fourThought": {
    "minThoughtsPerCategory": 1,
    "maxThoughtsPerCategory": 3,
    "enableBeliefStaking": true,
    "enableCommunityConsensus": true,
    "uncertaintyThresholds": {
      "veryLow": 0.1,
      "low": 0.3,
      "medium": 0.5,
      "high": 0.7,
      "veryHigh": 0.9
    }
  },
  "semanticLedger": {
    "embeddingDimensions": 100,
    "enableVectorSearch": true,
    "searchResultLimit": 15,
    "enableIntentAnalysis": true,
    "storageFormat": "hybrid"
  },
  "visualization": {
    "enableInteractiveCharts": true,
    "enableAnimations": true,
    "defaultChartLibrary": "plotly",
    "animationFrameRate": 30,
    "colorSchemes": {
      "trust": {
        "high": "#4CAF50",
        "medium": "#FFC107",
        "low": "#F44336"
      },
      "thoughtTypes": {
        "prediction": "#90CAF9",
        "reflection": "#A5D6A7",
        "statement": "#FFCC80",
        "question": "#CE93D8"
      },
      "valence": {
        "positive": "#4CAF50",
        "neutral": "#9E9E9E",
        "negative": "#F44336"
      }
    }
  },
  "workflow": {
    "maxConcurrentStages": 3,
    "stageTimeoutMs": 60000,
    "enableStageMetrics": true,
    "enableWorkflowVisualization": true,
    "retryFailedStages": true,
    "maxRetries": 2
  },
  "output": {
    "baseDirectory": "output",
    "enableDetailedLogging": true,
    "logLevel": "info",
    "saveIntermediateResults": true,
    "compressOutputFiles": false
  },
  "advanced": {
    "enableTemporalEmbeddings": true,
    "enableSocialProof": true,
    "enableProphetIncentives": true,
    "enableRegenerativeWisdom": true,
    "enableMultiDimensionalValence": true,
    "enableCommunityConsensus": true,
    "enableEpistemicNetworks": true,
    "enableBeliefRevisionTracking": true,
    "enableMetacognitiveMonitoring": true,
    "enableDialecticSynthesis": true,
    "enableCognitiveBiasDetection": true,
    "enableArgumentationFrameworks": true,
    "enableKnowledgeGraphs": true,
    "enableAdvancedSearchStrategies": true,
    "enableQueryAnalytics": true,
    "enableDomainClustering": true,
    "enableCrossDomainTransfer": true,
    "enableMultiAgentSynchronization": true
  },
  "analytics": {
    "enableComprehensiveAnalytics": true,
    "enableRealTimeMetrics": true,
    "enablePerformanceTracking": true,
    "enableUsageAnalytics": true,
    "enableBiasMonitoring": true,
    "enableCoherenceAssessment": true,
    "enableTransferLearningMetrics": true
  },
  "knowledge_management": {
    "embeddingDimensions": 100,
    "enableVectorSearch": true,
    "enableSemanticSearch": true,
    "enableEntitySearch": true,
    "enableKnowledgeGraphs": true,
    "enableTemporalIndexing": true,
    "enableDomainClustering": true,
    "maxQueryHistory": 1000,
    "searchResultLimit": 15,
    "similarityThreshold": 0.3,
    "enableIntentAnalysis": true,
    "enableQueryCategorization": true,
    "enableRelatedEntityDiscovery": true
  },
  "cognitive_architecture": {
    "enableDialecticReasoning": true,
    "enableMetacognition": true,
    "enableEpistemicReasoning": true,
    "enableArgumentationTheory": true,
    "enableBeliefDynamics": true,
    "enableWisdomAccumulation": true,
    "dialecticCycleThreshold": 0.7,
    "metacognitiveUpdateRate": 0.1,
    "epistemicNetworkDepth": 3,
    "argumentationValidityThreshold": 0.6
  },
  "trust_mechanisms": {
    "enableAdvancedTrustCalculation": true,
    "enableSocialProof": true,
    "enableTemporalTrustWeighting": true,
    "enableProphetIncentives": true,
    "enableCommunityConsensus": true,
    "enableBeliefStaking": true,
    "enableTrustPropagation": true,
    "trustUpdateRate": 0.1,
    "consensusThreshold": 0.7,
    "stakeWeightingFactor": 0.2,
    "socialProofWeight": 0.2,
    "temporalWeight": 0.05
  },
  "visualization": {
    "enableInteractiveCharts": true,
    "enableAnimations": true,
    "enableRealTimeUpdates": true,
    "enable3DVisualizations": true,
    "enableNetworkGraphs": true,
    "enableTimelineViews": true,
    "enableHeatmaps": true,
    "enableDashboardViews": true,
    "defaultChartLibrary": "plotly",
    "animationFrameRate": 30,
    "colorSchemes": {
      "trust": {
        "high": "#4CAF50",
        "medium": "#FFC107",
        "low": "#F44336"
      },
      "thoughtTypes": {
        "prediction": "#90CAF9",
        "reflection": "#A5D6A7",
        "statement": "#FFCC80",
        "question": "#CE93D8"
      },
      "valence": {
        "positive": "#4CAF50",
        "neutral": "#9E9E9E",
        "negative": "#F44336"
      },
      "cognitiveBiases": {
        "confirmation_bias": "#FF6B6B",
        "anchoring_bias": "#4ECDC4",
        "availability_heuristic": "#45B7D1"
      }
    }
  },
  "performance": {
    "maxConcurrentAgents": 10,
    "memoryLimitMB": 1024,
    "processingTimeoutMs": 30000,
    "enablePerformanceMonitoring": true,
    "enableMemoryOptimization": true,
    "enableCaching": true,
    "cacheSizeLimit": 1000,
    "enableLazyLoading": true,
    "enableBackgroundProcessing": true
  },
  "integration": {
    "enableExternalAPIs": true,
    "enableWebhookSupport": true,
    "enablePluginSystem": true,
    "enableDataConnectors": true,
    "enableExportCapabilities": true,
    "supportedFormats": ["json", "csv", "xml", "rdf", "jsonld"],
    "enableRealTimeSync": true,
    "syncIntervalMs": 5000,
    "enableConflictResolution": true
  }
}
