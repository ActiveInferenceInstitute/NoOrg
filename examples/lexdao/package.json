{
  "name": "lexdao-governance-workflow",
  "version": "1.0.0",
  "description": "Comprehensive LexDAO governance workflow implementation demonstrating multi-agent systems for decentralized legal engineering",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/lexdao_workflow.js",
    "dev": "ts-node lexdao_workflow.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint lexdao_workflow.ts",
    "lint:fix": "eslint lexdao_workflow.ts --fix",
    "demo": "npm run build && npm run start",
    "visualize": "python3 lexdao_visualizer.py --output-dir ../../output",
    "full-demo": "npm run demo && npm run visualize",
    "clean": "rm -rf dist output",
    "prepare": "npm run build"
  },
  "keywords": [
    "lexdao",
    "governance",
    "blockchain",
    "dao",
    "multi-agent",
    "workflow",
    "legal-engineering",
    "typescript",
    "decentralized"
  ],
  "author": "LexDAO Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lexDAO/NoOrg"
  },
  "bugs": {
    "url": "https://github.com/lexDAO/NoOrg/issues"
  },
  "homepage": "https://github.com/lexDAO/NoOrg/tree/main/examples/lexdao",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "openai": "^4.20.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "files": [
    "dist",
    "README.md",
    "lexdao_workflow.ts",
    "lexdao_constitution.md",
    "lexdao_visualizer.py",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "lexdao_workflow.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
}
